{
    "exclude": [
        "**/all-stubs",
        "board",
        "minified",
    ],
    "ignore": [
        "tests/mocks",
        "tests/data",
        "tests/codemods",
        "**/cst-test",
    ],
    "include": [
        "src",
        "tests"
    ],
    "verboseOutput": true,
    "executionEnvironments": [
        {
            "root": "board",
            "extraPaths": [
                "board/lib",
                //"all-stubs/cpython_core-pycopy",
                "all-stubs/cpython_core-micropython",
                "all-stubs/micropython-v1_18-frozen/esp32/RELEASE",
                "all-stubs/micropython-v1_18-esp32",
                "all-stubs/micropython-v1_18-pyboard",
                "all-stubs/typings/fallback",
                //                "all-stubs/micropython-latest-docstubs",
            ],
        },
        {
            "root": "src",
        },
        {
            "root": "tests",
            "extraPaths": [
                "src",
            ]
        }
    ]
}