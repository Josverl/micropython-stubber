// https://github.com/microsoft/pyright/blob/main/docs/configuration.md
{
    "include": [
        "esp32",
        "pyboard",
        "rp2",
        "esp8266",
        "mpy-stdlib",
    ],
    "exclude": [],
    "ignore": [],
    "pythonVersion": "3.9",
    "pythonPlatform": "All",
    "verboseOutput": true,
    "reportMissingImports": "error",
    "reportMissingModuleSource": "warning",
    "reportMissingTypeStubs": "warning",
    "stubPath": "micropython-typings",
    "logLevel": "Trace",
    "extraPaths": [
    ],
    "executionEnvironments": [
        {
            "root": "validate-stdlib",
            "extraPaths": [
                "../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "docstubs",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-docstubs",
                "../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "esp32",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-frozen/esp32/GENERIC",
                "../all-stubs/micropython-v1_18-esp32",
                //"../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "loboris-esp32",
            "extraPaths": [
                "../all-stubs/loboris-v3_2_24-esp32",
                "../all-stubs/loboris-v3_2_24-frozen",
                "../all-stubs/loboris-v3_2_24-esp32",
                "../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "pyboard",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-frozen/stm32/GENERIC",
                "../all-stubs/micropython-v1_18-pyboard"
            ]
        },
        {
            "root": "rp2/nano_connect",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-frozen/rp2/ARDUINO_NANO_RP2040_CONNECT",
                "../all-stubs/micropython-v1_18-rp2",
                "../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "rp2",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-frozen/rp2/GENERIC",
                "../all-stubs/micropython-v1_18-rp2",
                "../all-stubs/typings/fallback",
            ]
        },
        {
            "root": "esp8266",
            "extraPaths": [
                "../all-stubs/micropython-v1_18-frozen/esp8266/GENERIC",
                "../all-stubs/micropython-v1_18-esp8266"
            ]
        },
    ]
}